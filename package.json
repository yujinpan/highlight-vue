{
  "name": "highlight-vue",
  "version": "1.1.5",
  "author": "yujinpan",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "src/components"
  ],
  "scripts": {
    "version": "npm run build",
    "postversion": "git push origin master && npm publish",
    "serve": "vue-cli-service serve",
    "build": "cross-env NODE_ENV=production rollupx && eslint --fix --no-ignore lib/**/*.d.ts",
    "build:example": "vue-cli-service build",
    "build:types": "gulp && eslint --fix dist/**/*.d.ts",
    "build:styles": "node ./build/styles.js",
    "lint": "vue-cli-service lint",
    "start": "npm run serve"
  },
  "dependencies": {
    "@babel/runtime": "^7.x",
    "@types/prismjs": "^1.26.0",
    "core-js": "^3.19.1",
    "prismjs": "^1.28.0",
    "style-inject": "^0.x",
    "vue": "^2.6.12",
    "vue-class-component": "^7.x",
    "vue-property-decorator": "^8.x",
    "vue-runtime-helpers": "^1.x"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-plugin-typescript": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "jasmine-core": "^3.6.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^4.0.2",
    "node-sass": "^4.12.0",
    "rollupx": "^1.3.14",
    "sass-loader": "^7.1.0",
    "typescript": "~3.9.3",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie 9"
  ],
  "bugs": {
    "url": "https://github.com/yujinpan/highlight-vue/issues"
  },
  "homepage": "https://github.com/yujinpan/highlight-vue#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:yujinpan/highlight-vue.git"
  }
}
